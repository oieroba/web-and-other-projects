.container
  .auth-form
    %h2 Sign Up
    .form-group
      %input.input-lg.form-control{placeholder: "First name", type: "email"}
    .form-group
      %input.input-lg.form-control{placeholder: "Last name", type: "email"}
    .form-group
      %input.input-lg.form-control{placeholder: "Email", type: "email"}
    .form-group
      %input.input-lg.form-control{placeholder: "Password", type: "password"}
    .form-group
      %input.input-lg.form-control{placeholder: "Repeat Password", type: "password"}
    %button.btn.btn-block.btn-lg.btn-primary{type: "submit"} Sign Up
